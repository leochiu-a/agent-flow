{
  "name": "agent-flow",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @agent-flow/web dev",
    "build": "pnpm -r build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "type-check": "pnpm -r type-check",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm -r build && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "typescript": "~5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "oxlint --fix",
      "oxfmt --write"
    ]
  }
}
